{
  "name": "ws-fastify",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "imports": {
    "#~/*.js": "./dist/*.js"
  },
  "main": "src/main.ts",
  "scripts": {
    "fastify:build": "yarn fastify:clean && yarn build_ts",
    "fastify:clean": "rimraf ./dist",
    "fastify:start": "yarn fastify:build && node dist/main.js",
    "build_ts": "tsc",
    "build_ts_dev": "tsc -p tsconfig.dev.json",
    "dev": "yarn nodemon --watch ./src --watch ./config --ext ts,yaml --exec \"yarn clean && yarn build_ts_dev && node ./dist/main.js\"",
    "format": "prettier --write ./src/**",
    "lint": "yarn lint_format && yarn lint_code",
    "lint_code": "eslint ./src",
    "lint_format": "prettier --check ./src"
  },
  "resolutions": {
    "zod-to-json-schema@^3.17.1": "patch:zod-to-json-schema@npm%3A3.21.4#./.yarn/patches/zod-to-json-schema-npm-3.21.4-43c2360dbd.patch",
    "zod-to-json-schema@^3.21.4": "patch:zod-to-json-schema@npm%3A3.21.4#./.yarn/patches/zod-to-json-schema-npm-3.21.4-43c2360dbd.patch"
  },
  "dependencies": {
    "@fastify/auth": "^4.4.0",
    "@fastify/autoload": "^5.8.0",
    "@fastify/basic-auth": "^5.1.0",
    "@fastify/cors": "^8.5.0",
    "@fastify/error": "^3.4.1",
    "@fastify/jwt": "^7.2.2",
    "@fastify/rate-limit": "^9.0.1",
    "@fastify/sensible": "^5.5.0",
    "@fastify/swagger": "^8.12.0",
    "@fastify/swagger-ui": "^1.10.1",
    "@fastify/under-pressure": "^8.3.0",
    "@node-rs/bcrypt": "^1.7.3",
    "camelcase-keys": "^9.0.0",
    "close-with-grace": "^1.2.0",
    "config": "^3.3.9",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "fast-jwt": "^3.3.1",
    "fastify": "^4.24.3",
    "fastify-jwt-jwks": "^1.1.3",
    "fastify-multer": "^2.0.3",
    "fastify-node-config": "^1.0.0",
    "fastify-plugin": "^4.5.1",
    "fastify-type-provider-zod": "^1.1.9",
    "form-data-encoder": "^4.0.2",
    "mime-types": "^2.1.35",
    "node-cache": "^5.1.2",
    "openapi3-ts": "^4.1.2",
    "openid-client": "^5.6.4",
    "pino-pretty": "^10.2.0",
    "redis": "^4.6.8",
    "slonik": "^34.3.0",
    "slonik-interceptor-query-benchmarking": "^1.3.10",
    "slonik-interceptor-query-logging": "^1.4.7",
    "snake-case": "^3.0.4",
    "snakecase-keys": "^5.5.0",
    "zod": "^3.22.4",
    "zod-form-data": "^2.0.2",
    "zod-to-json-schema": "^3.21.4"
  },
  "devDependencies": {
    "@tsconfig/esm": "^1.0.5",
    "@tsconfig/node20": "^20.1.2",
    "@tsconfig/strictest": "^2.0.2",
    "@types/config": "^3.3.3",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.11.25",
    "@types/pg": "^8.10.9",
    "@types/tap": "^15.0.11",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "camelcase": "^8.0.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-ignore-generated": "^3.1.0",
    "leasot": "^13.3.0",
    "nodemon": "^2.0.22",
    "npm-run-all2": "^6.1.2",
    "openapi-types": "^12.1.3",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "type-fest": "^4.6.0",
    "typescript": "~5.4.0",
    "vitest": "^1.3.1"
  }
}
